{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "ooId": {
      "type": "biginteger",
      "unique": false,
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "pid": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "price": {
      "type": "decimal",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "imagePath": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "gender": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::gender.gender"
    },
    "subCategory": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::subcategory.subcategory",
      "inversedBy": "products"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "products"
    },
    "rating": {
      "type": "integer",
      "max": 5,
      "min": 0,
      "required": false,
      "default": 0,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
